  # Firebase
pod 'Firebase/Messaging'
pod 'Firebase/Analytics'
pod 'Firebase/Database'
pod 'Firebase/Auth'

  # Networking
pod 'Alamofire'

  # RXSwift
pod 'RxSwift'
pod 'RxDataSources'
pod 'RxSwiftExt'
pod 'RxGesture'

  # Usage Imports
pod 'JMMaskTextField-Swift'
pod 'SDWebImage'

  # SwiftLint
pod 'SwiftLint'

  target 'Quick Wait XibTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Quick Wait XibUITests' do
    # Pods for testing
  end

end
